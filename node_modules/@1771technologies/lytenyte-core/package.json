{
  "name": "@1771technologies/lytenyte-core",
  "description": "Blazingly fast headless React data grid with 100s of features.",
  "license": "Apache-2.0",
  "keywords": [
    "react",
    "data-grid",
    "react-table",
    "headless",
    "virtualized"
  ],
  "version": "1.0.18",
  "type": "module",
  "files": [
    "dist",
    "main.css",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://1771technologies.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/1771-technologies/lytenyte.git",
    "directory": "packages/lytenyte-core"
  },
  "bugs": {
    "url": "https://github.com/1771-technologies/lytenyte/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./icons": {
      "types": "./dist/icons/index.d.ts",
      "import": "./dist/icons/index.js",
      "require": "./dist/icons/index.js"
    },
    "./types": {
      "types": "./dist/+types.d.ts",
      "import": "./dist/+types.js",
      "require": "./dist/+types.js"
    },
    "./yinternal": {
      "types": "./dist/__internal.d.ts",
      "import": "./dist/__internal.js",
      "require": "./dist/__internal.js"
    },
    "./grid.css": "./main.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@1771technologies/lytenyte-shared": "1.0.18"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}